{
  "functions": {
    "source": "dist/functions",
    "predeploy": [
      "yarn clean",
      "yarn typecheck:app",
      "yarn build:functions",
      "yarn build:sitemap",
      "yarn build:app",
      "yarn copy:deps",
      "yarn install:deps"
    ]
  },
  "hosting": {
    "site": "data-sunlight-146313",
    "public": "dist/public",
    "predeploy": "yarn run-s build:public copy:next",
    "rewrites": [
      {
        "source": "**/!(api)",
        "function": "favware"
      },
      {
        "source": "/api{,/**}",
        "run": {
          "serviceId": "graphql-pokemon",
          "region": "europe-west1"
        }
      }
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=31536000"
          }
        ]
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
