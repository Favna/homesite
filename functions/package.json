    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "main": "lib/index.js",
  "dependencies": {
    "copy-dir": "^0.4.0",
    "del": "^4.0.0",
    "firebase-admin": "~7.0.0",
    "firebase-functions": "^2.2.0"
  },
  "devDependencies": {
    "@firebase/app-types": "^0.3.7",
    "nodemon": "^1.18.10",
    "typescript": "^3.2.2"
  },
  "private": true
}
