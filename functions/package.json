{
  "name": "functions",
  "engines": {
    "node": "8"
  },
  "scripts": {
    "build": "node cp-angular && tsc",
    "serve": "npm run build && firebase serve --only functions",
    "build-and-serve": "tsc & firebase serve --only functions",
    "servemon": "nodemon --exec yarn build-and-serve --watch src -e ts",
    "shell": "npm run build && firebase functions:shell",
