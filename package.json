{
  "name": "@favware/website",
  "version": "1.0.0",
  "description": "For hearth and home",
  "author": "@favware",
  "license": "MIT",
  "homepage": "https://favware.tech",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favware/website.git"
  },
  "bugs": {
    "url": "https://github.com/favware/website/issues"
  },
  "engines": {
    "node": "10"
  },
  "scripts": {
    "lint": "eslint src --fix --ext ts,tsx",
    "up": "yarn upgrade-interactive --latest",
    "dev": "next dev src",
    "prebuild": "yarn build:sitemap",
    "build": "next build src",
    "start": "next start src",
    "export": "next export src",
    "preserve": "run-s clean build:**",
    "clean": "rimraf src/.next",
    "build:sitemap": "node scripts/sitemapGenerator.js",
    "style": "prettier --write --loglevel=warn src/**/*.{ts,tsx,html,json,xml}",
    "netlify:build": "run-s build export",
    "docker:build": "docker build -t favware/website:latest .",
    "docker:run": "docker container run -d -it --name favware-website -p 2114:2114 favware/website:latest",
    "docker:clean": "docker container stop favware-website && docker container rm favware-website && docker rmi -f favware/website"
  },
  "dependencies": {
    "@klasa/utils": "^0.0.8",
    "@material-ui/core": "^4.10.1",
    "@material-ui/icons": "^4.9.1",
    "@skyra/discord-components-core": "^1.1.1",
    "@skyra/discord-components-react": "^1.1.1",
    "clsx": "^1.1.1",
    "mdi-react": "^7.3.0",
    "next": "^9.4.4",
    "next-seo": "^4.5.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-emoji-render": "^1.2.4",
    "react-ga": "^3.0.0",
    "react-if": "^3.4.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@favware/eslint-config-react-app": "0.1.0-alpha.0",
    "@favware/ts-config": "^3.0.0",
    "@rws-air/commitizen-config": "^2.0.1",
    "@rws-air/commitlint-config": "^2.0.1",
    "@types/node": "^14.0.11",
    "@types/react": "^16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-gtm-module": "^2.0.0",
    "cpx": "1.5.0",
    "cross-env": "7.0.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "nextjs-sitemap-generator": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "3.0.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.9.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@rws-air"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@rws-air/commitizen-config"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix --ext ts,tsx"
    ],
    "*.{html,babelrc,js,ts,tsx}": [
      "prettier --write"
    ]
  },
  "resolutions": {
    "acorn": "^6.4.1",
    "minimist": "^1.2.2",
    "kind-of": "^6.0.3"
  }
}
